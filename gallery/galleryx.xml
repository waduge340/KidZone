<?xml version="1.0" encoding="utf-8"?>
<gallery>
	<galleryName>Zen Gallery</galleryName>
	<settings>
		<initialState>Show Thumbnails</initialState>

		<imageScaleMode>fill</imageScaleMode>
		<useRotation>true</useRotation>
		<rotationDirection>Horizontal CW</rotationDirection>
		<rotationDuration>600</rotationDuration>
		<slideshowDelay>4</slideshowDelay>

		<frameSize>5</frameSize>
		<frameColor>0x000000</frameColor>
		<cornerRadius>7</cornerRadius>
		<backgroundColor>0xFFFFFF</backgroundColor>
		<dropShadow>true</dropShadow>

		<showButtons>Always</showButtons>
		<showAlbumsButton>true</showAlbumsButton>
		<showThumbnailsButton>true</showThumbnailsButton>
		<showSlideshowButton>true</showSlideshowButton>
		<showNavigationButton>true</showNavigationButton>
		<buttonsBackgroundAlpha>41</buttonsBackgroundAlpha>
		<buttonsBackgroundCornerRadius>6</buttonsBackgroundCornerRadius>

		<iconWidth>90</iconWidth>
		<iconHeight>55</iconHeight>
		<iconPadding>12</iconPadding>
		<panelBackgroundAlpha>43</panelBackgroundAlpha>

		<showCaption>Always</showCaption>
		<captionBackgroundAlpha>41</captionBackgroundAlpha>
		<captionBackgroundCornerRadius>6</captionBackgroundCornerRadius>

		<overSound>sounds/over.mp3</overSound>
		<clickSound>sounds/click.mp3</clickSound>
	</settings>
